<h1>Result : <%= @survey.name %></h1>

<ol>
  <% for question in @survey.questions %>
  <li><%= h question.content %> Is correct? <%= question.is_correct %></li>
  <ul>
    <% for answer in question.answers %>
      <li>	<%= h answer.content %>
	<% if answer.user_answer == true %>
		<i class="icon-ok icon-green"></i>
	<% end %>
      </li>
    <% end %>
  </ul>
  <% end %>
</ol>
